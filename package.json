{
  "name": "outfai",
  "private": true,
  "scripts": {
    "dev": "cd apps/web && next dev --turbo",
    "build": "cd apps/web && next build",
    "start": "cd apps/web && next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "db:doc": "npx tsx scripts/update-supabase-structure.ts",
    "gen:db-docs": "npx tsx scripts/update-supabase-structure.ts",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm run test && npm run build",
    "prepare": "husky",
    "docker:dev": "docker compose up --build",
    "docker:dev:detach": "docker compose up --build -d",
    "docker:dev:down": "docker compose down",
    "docker:dev:logs": "docker compose logs -f web",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "start:local": "powershell -ExecutionPolicy Bypass -File scripts/start.ps1",
    "stop:local": "powershell -ExecutionPolicy Bypass -File scripts/stop.ps1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
